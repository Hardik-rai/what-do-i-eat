#include <stdio.h> 
int main() 
{ 
  int x;
  printf(" 1.Pizza, Rs 239\n 2.Burger, Rs 129\n 3..Pasta, Rs 179\n 4.French Fries, Rs 99\n 5.Sandwich, Rs 149\n");
  scanf("%d",&x);
 switch(x) 
 { 
 case 1: 
 printf("Food item - Pizza\nPrice - Rs 239\n");
 break; 
 case 2: 
 printf("Food item - Burger\nPrice - Rs 129\n"); 
 break;
 case 3: 
 printf("Food item - Pasta\nPrice - Rs 179\n"); 
 break; 
 case 4: 
 printf("Food item - French Fries\nPrice - Rs 99\n"); 
 break;
 case 5: 
 printf("Food item - Sandwich\nPrice - Rs 149\n"); 
 break;
 default:
 printf("Wrong choice");
 }
  return 0; 
}
